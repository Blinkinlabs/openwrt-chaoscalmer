/dts-v1/;

/include/ "mt7628an.dtsi"

/ {
	compatible = "VoCore2", "mediatek,mt7628an-soc";
	model = "VoCore2";


	memory@0 {
		device_type = "memory";
		reg = <0x0 0x4000000>;
	};
	
	chosen {
		bootargs = "console=ttyS2,115200";
	};


	pinctrl {
		state_default: pinctrl0 {
			gpio {
				ralink,group = "i2c";
				ralink,function = "gpio";
			};
			agpio {
				ralink,group = "agpio";
				ralink,function = "uart2";
			};
		};
		uart2_pins: uart2 {
			uart2  {
				ralink,group = "spis";
				ralink,function = "pwm";
			};
		};
	};
	
	palmbus@10000000 {
		spi@b00 {
			status = "okay";

			m25p80@0 {
				#address-cells = <1>;
				#size-cells = <1>;
				compatible = "en25q64";
				reg = <0 0>;
				linux,modalias = "m25p80", "en25q64";
				spi-max-frequency = <10000000>;
				m25p,chunked-io = <32>;

				partition@0 {
					label = "u-boot";
					reg = <0x0 0x30000>;
					read-only;
				};

				partition@30000 {
					label = "u-boot-env";
					reg = <0x30000 0x10000>;
				};

				factory: partition@40000 {
					label = "factory";
					reg = <0x40000 0x10000>;
					read-only;
				};

				partition@50000 {
					label = "firmware";
					reg = <0x50000 0xfb0000>;
				};
			};

		};
		uartlite@c00 {
		};
		uart1@d00 {
			status = "okay";
		};
		uart2@e00 {
			pincrtl =  <&uart2_pins>;
			status = "okay";
		};
	};
};
